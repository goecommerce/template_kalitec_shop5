<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Template isFullResponsive="true">
    <Name>goeKalitec</Name>
    <Author>go eCommerce GmbH</Author>
    <URL>https://www.go-ecommerce.de</URL>
    <Parent>NOVA</Parent>
    <Preview>preview.png</Preview>
    <Version>5.3.1</Version>
    <MinShopVersion>5.4.0</MinShopVersion>
    <Description>Dieses Template dient als Vorlage f√ºr eigenes Child-Template des NOVA.</Description>
    <Settings>
        <Section Name="Theme" Key="theme">
            <Setting Description="Theme" Key="theme_default" Type="select" Value="kalitec" override="true">
                <Option Value="kalitec">Kalitec</Option>
            </Setting>
        </Section>
        <Section Name="kalitec-Footer" Key="kalitecfootersections">
            <Setting Description="Footer Logo Text" Key="fltext" Type="textarea" Value="Kalitec Verbindungstechnik GmbH Lindgesfeld 8h 42653 Solingen"/>
        </Section>
    </Settings>
    <Minify>
        <CSS Name="kalitec.css">
            <File Path="themes/kalitec/line-awesome.css"/>
            <File Path="themes/kalitec/kalitec.css"/>
            <File Path="themes/kalitec/animate.css"/>
            <File Path="themes/kalitec/custom.css"/>
        </CSS>
        <JS Name="jtl3.js">
            <File Path="js/jtl.evo.js" override="true" />
            <File Path="js/jtl.basket.js" override="true" />
            <File Path="js/wow.min.js" override="true" />
            <File Path="js/matchHeight.js" override="true" />
            <File Path="js/kalitec.js" override="true" />
            
        </JS>
    </Minify>
    <Boxes>
        <Container Position="left" Available="1" />
        <Container Position="right" Available="0" />
        <Container Position="top" Available="0" />
        <Container Position="bottom" Available="1" />
    </Boxes>
</Template>
